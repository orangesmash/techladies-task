<% provide(:title, "Home") %>
<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %> | Welcome, Techladies! </title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="header">
     <div class="row">
        <div class="col-xs-6"><%= link_to 'A Techladies Project', root_path, class: 'fontsforweb_fontid_32957' %></div>
        <div class="col-xs-2"><%= link_to 'About Me', aboutme_path, id: 'font_buttons', class: 'btn btn-default btn-lg' %></div>
        <div class="col-xs-2"><%= link_to 'Contact', contact_path, id: 'font_buttons', class: 'btn btn-default btn-lg' %></div>
        <div class="col-xs-2"><%= link_to 'Blog', articles_path, id: 'font_buttons', class: 'btn btn-default btn-lg' %></div>
     </div>
</div>

<div class="alert-bar">
    <div class="row">
        <div class="col-xs-12">
            <p class="flash"><%= flash.notice %></p>
        </div>
    </div>
</div>



<div class="container-fluid" style="max-width:900px">
    <div class="row">
        <div id="main-content" class="col-xs-10" style="padding-left: 5px">
            <%= yield %>
        </div>
        <div class="col-xs-2">
        </div>
    </div>
</div>

</body>
</html>
